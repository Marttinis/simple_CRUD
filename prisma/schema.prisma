generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model cliente {
  id    Int       @id
  nome  String?   @db.VarChar(180)
  email String    @unique @db.VarChar(100)
  data  DateTime? @db.Date
}

model filmes {
  id        Int     @id
  titulo    String? @db.VarChar(100)
  ano       Int?
  categoria String? @db.VarChar(100)
  preco     Float?
}
